#include<stdio.h>
int main(){

        int num;
        printf("1. Leer datos\n");
        printf("2. Visualizar datos generales");
        printf("3. Visualizar coseno del m√°ximo\n4. Salir\nOpcion:");
        scanf("%d", &num);

        int i=0, j=0;
        int array[15];



        switch (num)
        {
        case 1:
                do{
                        printf("\n-Numero:");
                        scanf("%d", &array[i]);
                        i++;
                }while(i<15 && array[i] != -1 );
        case 2:
                for (k = 0; k < i; k++ ){
                         printf("\n-Numeros:");
                         printf("\n", array[k]);
                }


        }

        return 0;
}
